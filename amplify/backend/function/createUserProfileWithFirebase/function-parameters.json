{
  "permissions": {
    "storage": {
      "UserProfile:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "yorihandlers",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true,
      "env": "yori"
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "firebaseProjectId",
      "environmentVariableName": "FIREBASE_PROJECT_ID"
    },
    {
      "cloudFormationParameterName": "firebaseClientEmail",
      "environmentVariableName": "FIREBASE_CLIENT_EMAIL"
    },
    {
      "cloudFormationParameterName": "plaidClientId",
      "environmentVariableName": "PLAID_CLIENT_ID"
    },
    {
      "cloudFormationParameterName": "plaidEnv",
      "environmentVariableName": "PLAID_ENV"
    }
  ],
  "secretNames": [
    "FIREBASE_PRIVATE_KEY",
    "PLAID_SECRET"
  ]
}