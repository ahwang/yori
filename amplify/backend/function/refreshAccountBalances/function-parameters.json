{
  "permissions": {
    "api": {
      "yori": [
        "Query",
        "Mutation"
      ]
    },
    "function": {
      "syncPlaidAccounts": [
        "create",
        "read",
        "update",
        "delete"
      ],
      "calculateFinancialSummary": [
        "create",
        "read",
        "update",
        "delete"
      ]
    },
    "storage": {
      "ConnectedAccount:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ],
      "PlaidToken:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ],
      "FinancialSummary:@model(appsync)": [
        "create",
        "read",
        "update",
        "delete"
      ]
    }
  },
  "lambdaLayers": [
    {
      "type": "ProjectLayer",
      "resourceName": "yorihandlers",
      "env": "dev",
      "version": "Always choose latest version",
      "isLatestVersionSelected": true
    }
  ],
  "environmentVariableList": [
    {
      "cloudFormationParameterName": "plaidClientId",
      "environmentVariableName": "PLAID_CLIENT_ID"
    },
    {
      "cloudFormationParameterName": "plaidEnv",
      "environmentVariableName": "PLAID_ENV"
    }
  ],
  "secretNames": [
    "PLAID_SECRET"
  ]
}